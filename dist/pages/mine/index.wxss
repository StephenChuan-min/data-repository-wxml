.index-wrapper{-webkit-box-sizing:border-box;box-sizing:border-box;min-height:100vh;background-color:#f6f7f9}.index-wrapper .navigationBar{height:380rpx;overflow:hidden;background:no-repeat url(data:image/png;base64,);background-size:cover}.index-wrapper .navigationBar-title{font-size:34rpx;font-weight:700;color:#fff;text-align:center}.index-wrapper .info-card{border-top:.2rpx solid transparent;margin:0 auto;-webkit-transform:translateY(-90rpx);-ms-transform:translateY(-90rpx);transform:translateY(-90rpx);width:690rpx;height:232rpx;background:#fff;-webkit-box-shadow:0 4rpx 24rpx 0 rgba(0,54,85,.11);box-shadow:0 4rpx 24rpx 0 rgba(0,54,85,.11);border-radius:16rpx;text-align:center}.index-wrapper .info-card .avatar{margin-top:-90rpx}.index-wrapper .info-card .avatar image{width:180rpx;height:180rpx}.index-wrapper .info-card .name{font-size:36rpx;font-weight:700;color:#20242e}.index-wrapper .info-card .username{margin-top:12rpx;font-size:28rpx;color:#4e5566}.index-wrapper .logout-button{background:#fff;font-size:36rpx;color:#20242e;height:100rpx;line-height:100rpx;text-align:center}.index-wrapper-modal{position:fixed;bottom:0;background-color:rgba(0,0,0,.4);width:100%;z-index:999;height:100vh}.index-wrapper-modal .modal-content{width:630rpx;height:280rpx;background:#fff;border-radius:16rpx;margin:50vh 0 0 50%;-webkit-transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);transform:translate(-50%,-50%);text-align:center}.index-wrapper-modal .modal-content .body{height:190rpx;font-size:36rpx;font-weight:700;color:#333;line-height:190rpx}.index-wrapper-modal .modal-content .footer{height:90rpx;border-top:2rpx #d7d9df solid;display:-webkit-flex;display:-ms-flexbox;display:flex}.index-wrapper-modal .modal-content .footer view{width:50%;font-size:32rpx;line-height:88rpx}.index-wrapper-modal .modal-content .footer view.cancel{color:#7d8699;border-right:2rpx #d7d9df solid}.index-wrapper-modal .modal-content .footer view.confirm{color:#397ae7}