.login-wrapper{position:relative;height:100vh;background-color:#fff}.login-wrapper .loading{background-color:rgba(0,0,0,.4);margin:0 auto;padding:20rpx;position:absolute;top:50%;left:50%;-webkit-transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);transform:translate(-50%,-50%);z-index:10;border-radius:32rpx}.login-wrapper .loading image{width:160rpx;height:160rpx}.login-wrapper .navigationBar{height:540rpx;overflow:hidden;background:no-repeat url(data:image/png;base64,);background-size:cover}.login-wrapper .navigationBar-title{font-size:34rpx;font-weight:700;color:#fff;text-align:center}.login-wrapper .navigationBar .logo{width:170rpx;height:170rpx;margin:0 auto}.login-wrapper .navigationBar .logo image{width:100%;height:100%;margin-top:120rpx}.login-wrapper .form-content{padding-top:80rpx;position:relative;width:590rpx;margin:0 auto}.login-wrapper .form-content .form-item{margin-bottom:40rpx}.login-wrapper .form-content::before{position:absolute;top:-40rpx;left:50%;-webkit-transform:translateX(-50%);-ms-transform:translateX(-50%);transform:translateX(-50%);content:"";display:block;width:0;height:0;border-left:20rpx solid transparent;border-right:20rpx solid transparent;border-top:20rpx solid transparent;border-bottom:30rpx solid #fff}.login-wrapper .form-content .h5-input{position:relative;-webkit-box-sizing:border-box;box-sizing:border-box;padding-left:92rpx;width:590rpx;height:84rpx;border:2rpx solid #c5c7ce;border-radius:42rpx}.login-wrapper .form-content .h5-input.is-error{border:2rpx #f66 solid}.login-wrapper .form-content .is-error-msg{position:absolute;left:92rpx;color:#f66;font-size:24rpx}.login-wrapper .form-content .input-placeholder{font-size:30rpx;color:#b2b8c9}.login-wrapper .form-content .iconfont{position:absolute;left:34rpx;color:#7d8699;font-size:34rpx;line-height:84rpx}.login-wrapper .form-content .suffix{width:218rpx;height:84rpx;position:absolute;right:0;bottom:164rpx;border-radius:42rpx;z-index:9}.login-wrapper .form-content .suffix image{width:100%;height:100%;border-radius:42rpx}.login-wrapper .form-content .login-btn{font-size:32rpx;font-weight:700;color:#fff;line-height:84rpx;height:84rpx;margin-top:80rpx;border-radius:42rpx;background-color:#397ae7}.login-wrapper .form-content .login-btn[aria-disabled=true]{background-color:rgba(57,122,231,.4)}